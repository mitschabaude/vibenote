{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/sw.js", "dest": "/sw.js" },
    { "src": "/s/(.*)", "dest": "/share.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
